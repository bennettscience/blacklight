{
  "name": "Blacklight",
  "version": "1.1.1",
  "description": "Makes your tab into a digital blackout poetry canvas.",
  "permissions" : [
     "tabs", "activeTab", "http://*/*", "https://*/*"
  ],
  "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.12.0.js"],
      "run_at": "document_end"
  }],
 "browser_action": {
    "default_title": "Make blackout poetry.",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
