{
  "name": "Blackout",
  "version": "1.1.1",
  "description": "lets you summon evil.",
  "permissions" : [
    "activeTab", "tabs"
  ],
  "background" : {
    "scripts" : ["background.js"],
    "persistent" : true
  },
  "content_scripts": [{
      "matches": ["*://*.com/*"],
      // "css": [ "poems.css" ],
      "js": ["jquery-1.12.0.js"],
      "run_at": "document_end"
  }],
 "browser_action": {
    "default_icon": "icon.png"
    // "default_popup": "popup.html"
  },
  "manifest_version": 2
}
